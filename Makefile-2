all: hello
	
hello: main.o function1.o function2.o hello.o
	g++ main.o function1.o function2.o hello.o -o hello

main.o: main.cpp
	g++ -cpp main.cpp

function1.o: function1.cpp
	g++ -cpp function1.cpp

function2.o: function2.cpp
	g++ -cpp function2.cpp

hello.o: hello.cpp
	g++ -cpp hello.cpp

clean:
	rm -rf *.o hello
